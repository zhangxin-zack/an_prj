    <typeAliases>
        <package name="com.scorer.model.model1"/>
        <package name="com.scorer.model.model2"/>
        <package name="com.scorer.model.model_request"/>
        <package name="com.scorer.model.model_response"/>
    </typeAliases>
</configuration>